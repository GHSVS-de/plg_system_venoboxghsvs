{
  "$schema": "https://developer.joomla.org/schemas/json-schema/web_assets.json",
  "name": "{{name}}",
  "version": "{{version}}",
	"versionSub": "{{versionSub}}",
  "description": "{{description}}",
  "license": "{{licenseLong}}",
	"assets": [
		{
			"name": "{{name}}.all",
			"type": "preset",
			"dependencies": [
				"{{name}}.core#script",
				"{{name}}.core#style",
				"{{name}}.user#style"
			]
		},
		{
			"name": "{{name}}.core",
			"type": "script",
			"uri": "{{name}}/venobox/venobox.js",
			"version": "{{versionSub}}"
		},
		{
			"name": "{{name}}.core",
			"description": "Das weight nur, weil ich im Moment denke, ich h√§tte das gerne nach der template.css.",
			"type": "style",
			"uri": "{{name}}/venobox/venobox.css",
			"weight": "50",
			"version": "{{versionSub}}"
		},
		{
			"name": "{{name}}.user",
			"description": "Custom CSS file TEMPLATE-ASSETS/css/venobox.css.",
			"type": "style",
			"weight": 500,
			"uri": "venobox.css"
		}
	]
}
